{
	"name": "ST",
	"description": "Scraping Tecnom",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["storage", "activeTab", "scripting"],
	"action": {
	  "default_icon": {
      "300": "/images/300.png"
	  }
	},
	"icons": {
		"300": "/images/300.png"
	},
	"content_scripts": [{
    "matches": [
      "https://web.whatsapp.com/"
    ],
    "js": ["bootstrap.bundle.min.js", "content.js"],
	"css": ["bootstrap.css"]
  }]
}